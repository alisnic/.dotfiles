{
  "it.todo": {
    "prefix": "itp",
    "body": "it('${1:description}')"
  },
  "describe": {
    "prefix": "desc",
    "body": "describe('${1:description}', () => {\n\t${0}\n})"
  },
  "function docs": {
    "prefix": "jsdoc",
    "body": "/**\n * ${1}\n */"
  },
  "it asynchronous": {
    "prefix": "it",
    "body": "it('${1:description}', async () => {\n\t${0}\n})"
  },
  "it synchronous": {
    "prefix": "its",
    "body": "it('${1:description}', () => {\n\t${0}\n})"
  },
  "beforeEach": {
    "prefix": "mbe",
    "body": "beforeEach(() => {\n\t${0}\n})"
  },
  "afterEach": {
    "prefix": "mae",
    "body": "afterEach(() => {\n\t${0}\n})"
  },
  "arrow function": {
    "prefix": "afi",
    "body": "(${1}) => ${2:statement}"
  },
  "arrow function with body": {
    "prefix": "afb",
    "body": "(${1}) => {\n\t${0}\n}"
  },
  "console.log": {
    "prefix": "cl",
    "body": "console.log(${0})"
  }
}

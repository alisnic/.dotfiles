--exclude=.git
--exclude=log
--exclude=node_modules
--exclude=tmp
--exclude=*.css
--exclude=*.json
--exclude=*.svg
--exclude=public
--exclude=vendor
--languages=-html
--fields=+l

--regex-ruby=/(^|;)[ \t]*(class|module)[ \t]+([A-Z][[:alnum:]_]+::([A-Z][[:alnum:]_]+)+)/\4/c,class,constant/
--regex-ruby=/(^|[:;])[ \t]*([A-Z][[:alnum:]_]+) *=/\2/c,class,constant/
--regex-ruby=/^[ \t]*scope[ \t]*:([a-zA-Z0-9_]+)/\1/
--regex-ruby=/^[ \t]*attr_accessor[ \t]*:([a-zA-Z0-9_]+)/\1/
--regex-ruby=/^[ \t]*attr_reader[ \t]*:([a-zA-Z0-9_]+)/\1/

--langdef=CoffeeScript
--langmap=CoffeeScript:.coffee
--regex-CoffeeScript=/(^|=[ \t])*class ([A-Za-z.]+)( extends [A-Za-z.]+)?$/\2/c,class/
--regex-CoffeeScript=/^[ \t]*@?([A-Za-z.]+):.*[-=]>.*$/\1/f,function/
--regex-CoffeeScript=/^[ \t]*([A-Za-z.]+)[ \t]+=.*[-=]>.*$/\1/f,function/
--regex-CoffeeScript=/^[ \t]*([A-Za-z.]+)[ \t]+=[^-=>\n]*$/\1/v,variable/
